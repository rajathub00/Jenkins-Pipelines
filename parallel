pipeline {
	agent any

stages {
	stage ('Parallel'){
		parallel {
			stage	('Stage-1')
			{
				steps{
				echo "Welcome rajat!"
				sleep 5
				}
			}	
			stage	('Stage-2')
			{
				steps{
				echo "Keep smilig!"
				sleep 5
				}
			}
			stage	('Stage-3')
			{
				steps{
				echo "Keep Growing!!"
				sleep 5
				}
			}	
	

		}
	}
  }
}
